view Layout {
  __margin: raw("0 auto")
  maxWidth: 1100
  width: percentage(100)
  flexGrow: 1
  flexShrink: 1
}

view Row {
  flexDirection: row
  flexGrow: 1
  flexShrink: 1
}

view Content {
  __overflow: auto
  flexShrink: 1
  flexGrow: 1
  paddingTop: 30
  paddingLeft: 30
  paddingRight: 10
  paddingBottom: 30
}

view NavContent {
  flexGrow: 1
  maxWidth: 1000
  paddingBottom: 10
}