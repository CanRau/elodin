variant Status {
  Active
}

view Header {
  paddingTop: 7
  paddingBottom: 7
  backgroundColor: rgb(0 176 164)
  flexDirection: row
}

view NavItem {
  justifyContent: center
  paddingLeft: 10
  paddingRight: 10
}

text NavItemLink {
  fontSize: 18
  textDecorationLine: none
  color: white
  [@hover] {
    color: rgb(50 246 234)
  }
  [Status=Active] {
    color: rgb(150 246 234)
    [@hover] {
      color: rgb(50 246 234)
    }
  }
}